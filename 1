function yourPatch(x, y) {
	noFill();
  stroke(238);
  rect(x, y, 100, 100);
  
  a = color(65);
  stroke(5);
  fill(a);
  rect(x+30,y+30, 70,50);
  var c = color(255, 204, 0);
  fill(c); 
	noStroke(); 
	rect(x+30, y+20, 55, 55);
  var d = color('magenta');
  fill(d); 
  ellipse(x+25, y+25, 80, 80); 

}

background(255);

yourPatch(0, 0);
yourPatch(width-100, height-100);
